{
   "name": "simple-node-mvc-starter-project",
   "description": "Simple Node.js MVC starter project",
   "version": "0.0.0",
   "license": "MIT",
   "type": "module",
   "main": "server.js",
   "repository": "github:dnajs/simple-node-mvc-starter-project",
   "homepage": "https://github.com/dnajs/simple-node-mvc-starter-project",
   "bugs": "https://github.com/dnajs/simple-node-mvc-starter-project/issues",
   "docs": "https://github.com/dnajs/simple-node-mvc-starter-project#readme",
   "keywords": [
      "jamstack",
      "javascript",
      "mvc",
      "node"
   ],
   "jshintConfig": {
      "esversion": 9,
      "strict": "implied",
      "eqeqeq": true,
      "undef": true,
      "unused": true,
      "node": true,
      "browser": true,
      "jquery": true,
      "mocha": true,
      "globals": {
         "app": true,
         "fetchJson": true
      }
   },
   "scripts": {
      "pretest": "jshint *.js web-root spec",
      "test": "mocha spec"
   },
   "dependencies": {
      "express": "~4.17",
      "fetch-json": "~2.4",
      "lokijs": "~1.5"
   },
   "devDependencies": {
      "canvas": "~2.8",
      "dna.js": "~1.7",
      "jquery": "~3.6",
      "jsdom": "~16.6",
      "jshint": "~2.13",
      "mocha": "~9.0",
      "node-fetch": "~2.6",
      "nodemon": "~2.0",
      "pretty-print-json": "~1.0",
      "server-listening": "~0.2",
      "web-ignition": "~1.4"
   }
}
