{
   "name": "simple-node-mvc-starter-project",
   "description": "Simple Node.js MVC starter project",
   "version": "0.0.0",
   "license": "MIT",
   "private": true,
   "main": "server.js",
   "repository": "github:dnajs/simple-node-mvc-starter-project",
   "homepage": "https://github.com/dnajs/simple-node-mvc-starter-project",
   "bugs": "https://github.com/dnajs/simple-node-mvc-starter-project/issues",
   "docs": "https://github.com/dnajs/simple-node-mvc-starter-project#readme",
   "keywords": [
      "node",
      "mvc",
      "javascript"
   ],
   "jshintConfig": {
      "esversion": 6,
      "strict": "implied",
      "undef": true,
      "unused": true,
      "node": true,
      "browser": true,
      "jquery": true,
      "globals": {
         "app": true,
         "fetchJson": false
      }
   },
   "scripts": {
      "test": "jshint *.js web-root"
   },
   "dependencies": {
      "express": "~4.16",
      "fetch-json": "~2.2",
      "lokijs": "~1.5"
   },
   "devDependencies": {
      "dna.js": "~1.5",
      "jquery": "~3.3",
      "jshint": "~2.9",
      "pretty-print-json": "0.0",
      "web-ignition": "~1.0"
   }
}
